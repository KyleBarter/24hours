<%- include('../partials/header') %>

<ul>
    <% if (user) { %>
    <% activities.forEach(function(a) { %>
        <li><%= a.activity %> | <%= a.activityType %> | <%= a.time %> minutes<a href="/activities/<%= a._id %>"> Journal</a></li>
    <% }) %>
    <% } else { %> 
        <p>Please login to view your day at a glance</p>
    <% } %>
</ul>

